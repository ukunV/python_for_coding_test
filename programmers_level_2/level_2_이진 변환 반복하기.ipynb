   "source": [
    "def solution(s):\n",
    "    answer = [0, 0]\n",
    "    \n",
    "    while s != '1':\n",
    "        count_0 = s.count('0')\n",
    "        count_1 = s.count('1')\n",
    "        \n",
    "        answer[0] += 1\n",
    "        answer[1] += count_0\n",
    "        \n",
    "        s = bin(count_1)[2:]\n",
    "        \n",
    "    return answer"
   ]
  }
